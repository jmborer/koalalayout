<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xml" href="http://openide.netbeans.org/fs/filesystem.xsl"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="FormDesignerPalette">
        <folder name="_Layouts_only_for_internal_use_">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="net.java.dev.koalalayout.nbsupport.Bundle"/>
            <file name="KoalaLayout.palette_item" url="palette/KoalaLayout.palette_item">
                <attr name="position" intvalue="900"/>
            </file>
        </folder>
    </folder>
    <folder name="org-netbeans-api-project-libraries">
        <folder name="Libraries">
            <file name="koalalayout.xml" url="koalalayout_library.xml"/>
        </folder>
    </folder>
    
    <!-- form class loader type registration -->
    <folder name="org-netbeans-modules-form">
        <folder name="classloader">
            <folder name="system">
                <file name="KoalaLayoutClasses.txt" url="KoalaLayoutClasses.txt"/>
            </folder>
        </folder>
    </folder>
</filesystem>
